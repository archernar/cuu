gawk -f readme.awk cuu  > README.md
echo "<pre>" >> README.md
echo "~/.cuu.txt  can contain defaults for these settings" >> README.md
cat .cuu.txt >> README.md
echo "</pre>" >> README.md



echo "<pre>" >> README.md
echo "cuu command list" >> README.md
echo "  " >> README.md
cuu -c commandlist | sort | gawk '{printf "%s ", $0;if ((NR%4)==0) print "";}' | column -t >> README.md
echo "  " >> README.md
echo "  " >> README.md
cuu -c commands >> README.md
echo "</pre>" >> README.md

echo "<pre>" >> README.md
echo "  " >> README.md
date >> README.md
echo "</pre>" >> README.md


git add readme.awk
git add README.md 
git add cuu.awk
git add pushit
git add cuu

git commit -m "Update"
git push -u origin master

